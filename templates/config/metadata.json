                    "agendas-pages-by-year": {
                        "title": "Agenda pages per year by meeting type",
                        "db": "meetings",
                        "query": "select count(*) as meeting_count, meeting, strftime(\"%Y\", date) as year from agendas group by strftime(\"%Y\", date), meeting order by year",
                        "library": "vega-lite",
                        "display": {
                            "mark": {
                                "type": "bar",
                                "tooltip": true
